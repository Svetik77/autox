<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
		<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
	</configSections>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="ServiceSoap" />
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost:55065/Service.asmx" binding="basicHttpBinding" bindingConfiguration="ServiceSoap" contract="AutoXService.ServiceSoap" name="ServiceSoap" />
    </client>
  </system.serviceModel>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
<appSettings><add key="FeatureToggle.SampleFeatureToggle" value="false" /><!-- For Windows Phone 7 apps delete the above line, and add Application.Current.Resources.Add("SampleFeatureToggle", false); in your app.xaml.cs constructor --></appSettings><log4net>
		<appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender">
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%date [%thread] %-5level %logger [%ndc] - %message%newline" />
			</layout>
		</appender>
		<root>
			<level value="INFO" />
			<appender-ref ref="ConsoleAppender" />
		</root>
	</log4net></configuration>